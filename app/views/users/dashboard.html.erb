<div class="cards-container">
    <div class="cards">
      <div class="row">
        <% @bookings_realised.each do |booking_realised| %>
          <div class="col-12 col-sm-6 col-md-4 col-lg-3">
            <%= render 'users/mission', booking: booking_realised %>
          </div>
        <% end %>
        <% @bookings_to_come.each do |booking_to_come| %>
          <div class="col-12 col-sm-6 col-md-4 col-lg-3">
            <%= render 'users/mission', booking: booking_to_come %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
